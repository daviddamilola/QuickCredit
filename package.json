    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-sessions": "^1.0.6",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "node": "^12.1.0",
